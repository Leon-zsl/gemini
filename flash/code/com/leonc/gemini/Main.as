package com.leonc.gemini {import flash.display.MovieClip;import flash.events.Event;import flash.display.StageScaleMode;import flash.external.*;import scaleform.gfx.SystemEx;import scaleform.gfx.Extensions;import com.leonc.gemini.App;import com.leonc.gemini.C2F;import com.leonc.gemini.log.Log;public class Main extends MovieClip {	private var _app:App = new App();	private var _c2f:C2F = new C2F();	public function get c2f():C2F { return _c2f; }	public function get app():App { return _app; }	public function Main() 	{		super();		addEventListener( Event.ADDED_TO_STAGE, startup, false, 0, true );		Log.Info("main constructor...");	}		public function startup( e:Event ) 	{		removeEventListener( Event.ADDED_TO_STAGE, startup, false );		_c2f.init(this);		_app.startup(this);		addEventListener( Event.ENTER_FRAME, tick, false, 0, true );	}	public function shutdown():void	{		_app.shutdown();	}	public function tick( e:Event ):void 	{		_app.update();		Log.Info("this is main swf");	}}}